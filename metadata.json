{
  "name": "broadinstitute-certs",
  "version": "1.0.0",
  "author": [
    "Riccardo Calixte <rcalixte@broadinstitute.org>",
    "Andrew Teixeira <teixeira@broadinstitute.org"
  ],
  "description": "Module for SSL certificate configuration",
  "summary": "Configures and manages SSL certificate deployments, restarting services as configured.",
  "license": "Apache-2.0",
  "source": "git://github.com/broadinstitute/puppet-certs.git",
  "project_page": "https://github.com/broadinstitute/puppet-certs",
  "issues_url": "https://github.com/broadinstitute/puppet-certs/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "SuSE",
      "operatingsystemrelease": [
        "11SP4",
        "12SP1"
      ]
    }
  ],
  "dependencies": [
    { "name":"puppetlabs-stdlib","version_requirement":">= 4.0.0 < 5.0.0" },
    { "name":"puppetlabs/concat","version_requirement":">= 1.1.1 < 3.0.0" }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.0.0 < 5.0.0"
    }
  ]
}
